{
  "name": "brimu-backend",
  "version": "1.0.0",
  "description": "Backend do sistema Brimu - Gestão para Serviços Arbóreos",
  "main": "dist/app.js",
  "scripts": {
    "build": "node --max-old-space-size=4096 node_modules/typescript/bin/tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "dev:js": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "create-admin": "ts-node scripts/createAdmin.ts",
    "setup": "npm run create-admin"
  },
  "keywords": [
    "brimu",
    "arborismo",
    "gestao",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Equipe Brimu",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.10",
    "sharp": "^0.32.5",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.8.7",
    "@types/nodemailer": "^6.4.13",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.6.2",
    "mongodb-memory-server": "^9.1.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
