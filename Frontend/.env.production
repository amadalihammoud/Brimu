# Configurações do Frontend para Produção
VITE_API_URL=https://brimu-1.onrender.com
VITE_APP_NAME=Brimu
VITE_APP_VERSION=1.0.0

# Configurações de Upload
VITE_MAX_FILE_SIZE=10485760
