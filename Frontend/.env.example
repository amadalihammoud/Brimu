# Variáveis de ambiente para o projeto Brimu

# Google Analytics 4
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Analytics Endpoint para dados customizados (opcional)
REACT_APP_ANALYTICS_ENDPOINT=https://api.brimu.com.br/analytics

# Configurações do app
REACT_APP_NAME=Brimu
REACT_APP_URL=https://brimu-arborizacao.vercel.app
REACT_APP_DESCRIPTION=Serviços profissionais de arborização e paisagismo

# Configurações de desenvolvimento
REACT_APP_DEBUG_MODE=false
REACT_APP_ENABLE_ANALYTICS=true

# Configurações PWA
REACT_APP_PWA_ENABLED=true

# Configurações de cache
REACT_APP_CACHE_VERSION=1.0.0

# Configurações de acessibilidade
REACT_APP_CONTRAST_FIXES=true
REACT_APP_HIGH_CONTRAST_MODE=false

# Configurações de performance
REACT_APP_LAZY_LOADING=true
REACT_APP_WEB_VITALS_TRACKING=true